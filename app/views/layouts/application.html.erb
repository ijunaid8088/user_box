<!DOCTYPE html>
<html>
  <head>
    <title>UserBox</title>
    <%= csrf_meta_tags %>
    <%= yield :head %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, class: "alert alert-#{name}" %>
  <% end %>

    <%= yield %>
  </body>
</html>
